# Ejemplo de configuración completa para usar Gemini

# Provider de WhatsApp
provider=baileys

# Meta (solo si usas provider=meta)
jwtToken=
numberId=
verifyToken=

# ===== CONFIGURACIÓN DE IA =====

# Seleccionar proveedor de IA: "openai" o "gemini"
ai_provider=gemini

# OpenAI (requerido para notas de voz)
openai_apikey=sk-proj-xxxxxxxxxx
openai_model=gpt-3.5-turbo

# Google Gemini (AGREGAR TU API KEY AQUÍ)
gemini_apikey=AIzaSyxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
gemini_model=gemini-2.0-flash

# Configuración del servidor
PORT=3008

# Retrocompatibilidad
model=gpt-3.5-turbo