# Provider configuration (baileys or meta)
provider=baileys

# Meta WhatsApp Business API (only needed if using Meta provider)
jwtToken=YOUR_JWT_TOKEN_HERE
numberId=YOUR_PHONE_NUMBER_ID_HERE
verifyToken=YOUR_VERIFY_TOKEN_HERE

# AI Provider Selection (openai or gemini)
ai_provider=openai

# OpenAI Configuration
openai_apikey=YOUR_OPENAI_API_KEY_HERE
openai_model=gpt-3.5-turbo

# Google Gemini Configuration
gemini_apikey=YOUR_GEMINI_API_KEY_HERE
gemini_model=gemini-2.0-flash

# Backward compatibility (will use ai_provider and respective model)
model=gpt-3.5-turbo

# Server Configuration
PORT=3008